  \endverb
  \field{isbn}{9781118664322}
  \field{pages}{99\bibrangedash 110}
  \field{title}{Ocean Carbon Pumps: Analysis of Relative Strengths and
  Efficiencies in Ocean-Driven Atmospheric CO2 Changes}
  \field{year}{1985}
\endentry

