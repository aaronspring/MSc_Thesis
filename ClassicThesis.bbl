  \endverb
  \field{number}{21}
  \field{pages}{3043\bibrangedash 3057}
  \field{title}{{Synchronous Variability in the Southern Hemisphere Atmosphere,
  Sea Ice, and Ocean Resulting from the Annular Mode}}
  \field{volume}{15}
  \verb{file}
