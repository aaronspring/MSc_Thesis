  \verb 10.1111/j.1529-8817.1976.tb02866.x
  \endverb
  \field{issn}{1529-8817}
  \field{number}{4}
  \field{pages}{421\bibrangedash 430}
  \field{title}{{The relationship between photosynethsis and light for natural
  asseblages of coastal marine phytoplankton}}
  \field{volume}{12}
  \field{journaltitle}{Journal of Phycology}
  \field{year}{1976}
\endentry

