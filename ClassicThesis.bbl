  \verb 10.1175/1520-0442(2003)016<4134:TITSAM>2.0.CO;2
  \endverb
  \verb{eprint}
  \verb http://dx.doi.org/10.1175/1520-0442(2003)016<4134:TITSAM>2.0.CO;2
  \endverb
  \field{number}{24}
  \field{pages}{4134\bibrangedash 4143}
  \field{title}{Trends in the Southern Annular Mode from Observations and
  Reanalyses}
  \field{volume}{16}
